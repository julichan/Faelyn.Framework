<Project>
  <PropertyGroup>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat> 
    <PackageVersion Condition="'$(PackageVersion)' == ''">0.0.0-dev</PackageVersion>
    <Authors>Julichan</Authors>
    <Copyrights>Copyright (c) Julichan $([System.DateTime]::Now.ToString(yyyy))</Copyrights>
    <ProjectUrl>https://github.com/julichan/Faelyn.Framework</ProjectUrl>
    <RepositoryUrl>https://github.com/julichan/Faelyn.Framework</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>
</Project>