<Project>
  <PropertyGroup>
    <SolutionBuildPath>$(MSBuildThisFileDirectory)</SolutionBuildPath>
    <BaseIntermediateOutputPath>$(SolutionBuildPath)/tmp/obj/$(MSBuildProjectName)/</BaseIntermediateOutputPath>
    <BaseOutputPath>$(SolutionBuildPath)/tmp/bin/$(MSBuildProjectName)/</BaseOutputPath>
    <PackageInputPath>$(SolutionBuildPath)libs/</PackageInputPath>
    <PackageOutputPath>$(SolutionBuildPath)tmp/packages/</PackageOutputPath>
    <ContainerIntermediateOutputPath>$(SolutionBuildPath)/tmp/containers/$(MSBuildProjectName)/</ContainerIntermediateOutputPath>
  </PropertyGroup>

  <Import Project="BaseDirectory.User.props" Condition="Exists('BaseDirectory.User.props')" />
</Project>